{
  "name": "brer",
  "version": "0.0.0",
  "description": "Brer bindings for Node.js",
  "type": "module",
  "main": "./brer.cjs",
  "types": "./brer.d.ts",
  "exports": {
    ".": {
      "import": "./brer.mjs",
      "require": "./brer.cjs",
      "types": "./brer.d.ts"
    }
  },
  "files": [
    "brer.cjs",
    "brer.d.ts",
    "brer.mjs"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "standard",
    "prepublishOnly": "npm run lint && npm run build && npm test",
    "test": "c8 ava"
  },
  "keywords": [
    "brer"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "dependencies": {
    "got": "^12.5.3"
  },
  "devDependencies": {
    "ava": "^5.1.0",
    "c8": "^7.12.0",
    "nock": "^13.2.9",
    "rollup": "^3.5.0",
    "standard": "^17.0.0"
  }
}
