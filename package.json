{
  "name": "brer",
  "version": "0.12.0",
  "description": "Official Node.js runtime for Brer.",
  "type": "module",
  "main": "./brer.cjs",
  "types": "./brer.d.ts",
  "exports": {
    ".": {
      "import": "./brer.mjs",
      "require": "./brer.cjs",
      "types": "./brer.d.ts"
    }
  },
  "files": [
    "lib",
    "brer.cjs",
    "brer.mjs",
    "brer.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "standard",
    "prepublishOnly": "npm run lint && npm run build && npm test",
    "test": "c8 ava"
  },
  "keywords": [
    "brer",
    "serverless",
    "job",
    "task"
  ],
  "author": "Giacomo Gregoletto <giacomo.gregoletto@evologi.it>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brer/brer-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/brer/brer-nodejs/issues"
  },
  "dependencies": {
    "undici": "^7.2.3"
  },
  "devDependencies": {
    "ava": "^6.2.0",
    "c8": "^10.1.3",
    "rollup": "^4.30.1",
    "standard": "^17.1.2"
  }
}
